{
  "name": "typescript-sounding-board",
  "version": "0.0.1",
  "description": "workspace to try out typescript scripts and packages",
  "author": "Tiberius Silivestru <tiberius.silivestru@gmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "swc"
  ],
  "scripts": {
    "format": "prettier --check **/*.ts",
    "format:fix": "prettier --write **/*.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "node -r @swc-node/register index.ts",
    "script": "node -r @swc-node/register scripts/$npm_config_file",
    "test": "jest --"
  },
  "devDependencies": {
    "@jest/globals": "^29.2.2",
    "@swc-node/register": "^1.5.4",
    "@swc/core": "^1.3.10",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.5",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}
